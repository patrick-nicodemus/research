@inproceedings{richardsonSystemDescriptionProof1998,
  title = {System {{Description}}: {{Proof Planning}} in {{Higher-Order Logic}} with {{LambdaClam}}},
  booktitle = {Automated Deduction, {{CADE-15}}: 15th {{International Conference}} on {{Automated Deduction}}, {{Lindau}}, {{Germany}}, {{July}} 5-10, 1998: Proceedings},
  author = {Richardson, Julian and Smaill, Alan and Green, Ian},
